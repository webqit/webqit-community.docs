{
    "ENTRY_DIR": "./[name]",
    "OUTPUT_FILE": "./bundle.html",
    "ASSETS_STORAGE_BASE": "../../../../public/html/tooling/[name]",
    "ASSETS_PUBLIC_BASE": "/html/tooling/[name]",
    "MAX_DATA_URL_SIZE": 1024,
    "LOADERS": [
        {
            "name": "default:md-loader",
            "argument/flag": true,
            "args": {
                "base_url": "/tooling/",
                "code_highlighting": "1",
                "outline_generation":"create",
                "flavor": "github"
            }
        }
    ],
    "IGNORE_FOLDERS_BY_PREFIX": [
        "."
    ],
    "CREATE_OUTLINE_FILE": "create",
    "TEMPLATE_EXT": "",
    "MODULE_ID_ATTR": "name",
    "EXPORT_MODE": "attribute",
    "EXPORT_GROUP_ATTR": "exportgroup",
    "EXPORT_ELEMENT": "html-export",
    "EXPORT_ID_ATTR": "export",
    "loader": true,
    "__advanced": false
}